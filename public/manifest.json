{
  "manifest_version": 3,
  "name": "Notion Table of Contents (Dev)",
  "description": "Notion Table of Contents",
  "version": "1.1.0",
  "action": {
    "default_icon": { "19": "./images/icon.png" }
  },
  "background": {
    "service_worker": "./js/background.js"
  },
  "permissions": ["webNavigation", "scripting"],
  "host_permissions": ["https://www.notion.so/*"],
  "web_accessible_resources": [
    {
      "resources": ["images/*.png"],
      "matches": ["https://www.notion.so/*"]
    }
  ]
}
