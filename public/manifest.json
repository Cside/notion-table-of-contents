{
  "manifest_version": 3,
  "name": "Notion Table of Contents (Dev)",
  "description": "Notion Table of Contents",
  "version": "1.1.0",
  "action": {
    "default_icon": { "19": "images/icon.png" }
  },
  "content_scripts": [
    {
      "matches": ["https://www.notion.so/*"],
      "js": ["js/content-script.js"]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["webNavigation", "activeTab", "scripting"],
  "host_permission": ["https://www.notion.so/*"],
  "web_accessible_resources": [
    {
      "resources": ["images/*.png"],
      "matches": ["https://www.notion.so/*"]
    }
  ]
}
